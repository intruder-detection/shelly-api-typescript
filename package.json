    "dotenv-cli": "^7.4.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-prettier": "^5.0.0",
    "npx": "^10.2.2",
    "prettier": "^3.3.1",
    "ts-essentials": "^10.0.0",
