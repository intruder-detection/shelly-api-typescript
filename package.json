  "description": "Typescript client for the Shelly HTTP API",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/**"
  ],
  "scripts": {
    "debug_gen2": "dotenv -- npx tsx --inspect-brk test/gen2/main.ts",
